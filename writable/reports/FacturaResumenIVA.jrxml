	<parameter name="Serie" class="java.lang.String"/>
	<parameter name="Numero" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
  M.iva AS tipo_iva,
  M.re AS tipo_re,
  M.irpf AS tipo_irpf,
  SUM(M.total) AS base_imponible,
  SUM(M.total * (M.iva / 100)) AS cuota_iva,
  SUM(M.total * (M.re / 100)) AS cuota_re,
  SUM(M.total * (M.irpf / 100)) AS cuota_irpf
FROM esi_invoice_items M
JOIN esi_invoices I ON M.invoice_id = I.id
WHERE I.invoice_type = 0
  AND I.serie = $P{Serie}
  AND I.numero = $P{Numero}
  AND M.Deleted <> 1
GROUP BY M.iva, M.re, M.irpf 
ORDER BY M.iva, M.re, M.irpf]]>
	</queryString>
